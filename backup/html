	<script src="<?= base_url('sneebf')?>/javascript/homeuser.js"></script>
	<script src="<?= base_url('sneebf')?>/javascript/homeuser2.js"></script>
	<script src="<?= base_url('sneebf')?>/javascript/homeuser3.js"></script>
	<script src="<?= base_url('sneebf')?>/javascript/homeuser4.js"></script>
	<script src="<?= base_url('sneebf')?>/javascript/homeuser5.js"></script>
	<script>
		document.querySelector('.button-audio-media').addEventListener('click', function () {
			const wrapper = document.querySelector('.wrapper-media');
			const mediaAudio = document.querySelector('.media-audio');
			const backcharVi = document.querySelector('.background-char-media-video');
			const roVideo = document.querySelector('.rotate-video');
			const butVideo = document.querySelector('.button-video-media');

			// Move the wrapper
			wrapper.classList.toggle('translate1'); // Toggle translate class for the wrapper
			backcharVi.classList.toggle('display1');
			roVideo.classList.toggle('display1');
			butVideo.classList.toggle('disable1');

			// Show the media-audio with animation
			if (!mediaAudio.classList.contains('visible')) {
				mediaAudio.style.display = 'block'; // Ensure it's part of the DOM
				setTimeout(() => {
					mediaAudio.classList.add('visible'); // Add the visible class with delay for animation
				}, 10); // Small delay to allow the transition to work
			} else {
				mediaAudio.classList.remove('visible'); // Remove the visible class to hide
				setTimeout(() => {
					mediaAudio.style.display = 'none'; // Hide after animation completes
				}, 500); // Match the transition duration in CSS
			}
		});

	</script>

